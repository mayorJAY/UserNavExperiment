<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:dist="http://schemas.android.com/apk/distribution"
    package="com.example.josycom.usernavexperiment">

    <dist:module dist:instant="true" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.example.josycom.usernavexperiment.DonutActivity"
            android:label="Donut Activity"
            android:parentActivityName="com.example.josycom.usernavexperiment.MainActivity"/>
            <meta-data android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.josycom.usernavexperiment.MainActivity"/>
        <activity android:name="com.example.josycom.usernavexperiment.FroyoActivity"
            android:label="Froyo Activity"
            android:parentActivityName="com.example.josycom.usernavexperiment.MainActivity"/>
        <meta-data android:name="android.support.PARENT_ACTIVITY"
            android:value="com.example.josycom.usernavexperiment.MainActivity"/>
        <activity android:name="com.example.josycom.usernavexperiment.IceCreamActivity"
            android:label="IceCream Activity"
            android:parentActivityName="com.example.josycom.usernavexperiment.MainActivity"/>
        <meta-data android:name="android.support.PARENT_ACTIVITY"
            android:value="com.example.josycom.usernavexperiment.MainActivity"/>
        <activity
            android:name="com.example.josycom.usernavexperiment.MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>